{
  "require": {
    "adigital/cookie-consent-banner": "1.2.6",
    "barrelstrength/sprout-fields": "3.8.5",
    "bitboxde/minifier": "2.0.5",
    "craftcms/cms": "3.7.39",
    "craftcms/redactor": "2.10.7",
    "craftcms/store-hours": "2.2.0",
    "dukt/analytics": "4.0.10",
    "ether/logs": "3.0.5",
    "jalendport/craft-queuemanager": "1.2.0",
    "madedigital/image-helper": "^0.0.1",
    "marionnewlevant/twig-perversion": "2.2.0",
    "mblode/svgplaceholder": "1.0.3",
    "mmikkel/cp-field-inspect": "1.4.2",
    "monolog/monolog": "^2.2",
    "nystudio107/craft-seomatic": "3.4.30",
    "putyourlightson/craft-blitz": "3.12.2",
    "samdark/yii2-psr-log-target": "^1.1",
    "selvinortiz/patrol": "3.1.3",
    "solspace/craft-freeform": "3.13.7",
    "topshelfcraft/wordsmith": "3.3.0.1",
    "vlucas/phpdotenv": "^3.4.0",
    "wbrowar/adminbar": "3.1.11",
    "weareferal/remote-sync": "1.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.3" 
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "path",
      "url": "./plugins/*"
    }
  ]
}
